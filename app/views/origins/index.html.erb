<h1>Listing origins</h1>

<table>
  <thead>
    <tr>
      <th>File name</th>
      <th>File description</th>
      <th>Created in sprint</th>
      <th>Updated in sprint</th>
      <th>Abbreviation</th>
      <th>Base type</th>
      <th>Book mainframe</th>
      <th>Periodicity</th>
      <th>Periodicity details</th>
      <th>Data retention type</th>
      <th>Extractor file type</th>
      <th>Room 1 notes</th>
      <th>Mnemonic</th>
      <th>Cd5 portal source code</th>
      <th>Cd5 portal source name</th>
      <th>Cd5 portal target code</th>
      <th>Cd5 portal target name</th>
      <th>Hive table name</th>
      <th>Mainframe storage type</th>
      <th>Room 2 notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @origins.each do |origin| %>
      <tr>
        <td><%= origin.file_name %></td>
        <td><%= origin.file_description %></td>
        <td><%= origin.created_in_sprint %></td>
        <td><%= origin.updated_in_sprint %></td>
        <td><%= origin.abbreviation %></td>
        <td><%= origin.base_type %></td>
        <td><%= origin.book_mainframe %></td>
        <td><%= origin.periodicity %></td>
        <td><%= origin.periodicity_details %></td>
        <td><%= origin.data_retention_type %></td>
        <td><%= origin.extractor_file_type %></td>
        <td><%= origin.room_1_notes %></td>
        <td><%= origin.mnemonic %></td>
        <td><%= origin.cd5_portal_source_code %></td>
        <td><%= origin.cd5_portal_source_name %></td>
        <td><%= origin.cd5_portal_target_code %></td>
        <td><%= origin.cd5_portal_target_name %></td>
        <td><%= origin.hive_table_name %></td>
        <td><%= origin.mainframe_storage_type %></td>
        <td><%= origin.room_2_notes %></td>
        <td><%= link_to 'Show', origin %></td>
        <td><%= link_to 'Edit', edit_origin_path(origin) %></td>
        <td><%= link_to 'Destroy', origin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Origin', new_origin_path %>

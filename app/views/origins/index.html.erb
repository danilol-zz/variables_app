<h1>Origens</h1>

<table>
  <thead>
    <tr>
      <th><%= t('activerecord.attributes.origin.file_name') %></th>
      <th><%= t('activerecord.attributes.origin.file_description') %></td>
      <th><%= t('activerecord.attributes.origin.created_in_sprint') %></td>
      <th><%= t('activerecord.attributes.origin.updated_in_sprint') %></td>
      <th><%= t('activerecord.attributes.origin.abbreviation') %></td>
      <th><%= t('activerecord.attributes.origin.base_type') %></td>
      <th><%= t('activerecord.attributes.origin.book_mainframe') %></td>
      <th><%= t('activerecord.attributes.origin.periodicity') %></td>
      <th><%= t('activerecord.attributes.origin.periodicity_details') %></td>
      <th><%= t('activerecord.attributes.origin.data_retention_type') %></td>
      <th><%= t('activerecord.attributes.origin.extractor_file_type') %></td>
      <th><%= t('activerecord.attributes.origin.room_1_notes') %></td>
      <th><%= t('activerecord.attributes.origin.mnemonic') %></td>
      <th><%= t('activerecord.attributes.origin.cd5_portal_source_code') %></td>
      <th><%= t('activerecord.attributes.origin.cd5_portal_source_name') %></td>
      <th><%= t('activerecord.attributes.origin.cd5_portal_target_code') %></td>
      <th><%= t('activerecord.attributes.origin.cd5_portal_target_name') %></td>
      <th><%= t('activerecord.attributes.origin.hive_table_name') %></td>
      <th><%= t('activerecord.attributes.origin.mainframe_storage_type') %></td>
      <th><%= t('activerecord.attributes.origin.room_2_notes') %></td>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @origins.each do |origin| %>
      <tr>
        <td><%= origin.file_name %></td>
        <td><%= origin.file_description %></td>
        <td><%= origin.created_in_sprint %></td>
        <td><%= origin.updated_in_sprint %></td>
        <td><%= origin.abbreviation %></td>
        <td><%= origin.base_type %></td>
        <td><%= origin.book_mainframe %></td>
        <td><%= origin.periodicity %></td>
        <td><%= origin.periodicity_details %></td>
        <td><%= origin.data_retention_type %></td>
        <td><%= origin.extractor_file_type %></td>
        <td><%= origin.room_1_notes %></td>
        <td><%= origin.mnemonic %></td>
        <td><%= origin.cd5_portal_origin_code %></td>
        <td><%= origin.cd5_portal_origin_name %></td>
        <td><%= origin.cd5_portal_destination_code %></td>
        <td><%= origin.cd5_portal_destination_name %></td>
        <td><%= origin.hive_table_name %></td>
        <td><%= origin.mainframe_storage_type %></td>
        <td><%= origin.room_2_notes %></td>
        <td><%= link_to t('show'), origin %></td>
        <td><%= link_to t('edit'), edit_origin_path(origin) %></td>
        <td><%= link_to t('destroy'), origin, method: :delete, data: { confirm: t('are_you_sure') } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nova Origem', new_origin_path %>

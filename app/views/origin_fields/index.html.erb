<h1>Listing origin_fields</h1>

<table>
  <thead>
    <tr>
      <th>Field name</th>
      <th>Origin pic</th>
      <th>Data type origin field</th>
      <th>Fmbase format type</th>
      <th>Generic data type</th>
      <th>Decimal origin field</th>
      <th>Mask origin field</th>
      <th>Position origin field</th>
      <th>Width origin field</th>
      <th>Is key</th>
      <th>Will use</th>
      <th>Has signal</th>
      <th>Room 1 notes</th>
      <th>Cd5 variable number</th>
      <th>Cd5 output order</th>
      <th>Cd5 variable name</th>
      <th>Cd5 origin format</th>
      <th>Cd5 origin format desc</th>
      <th>Cd5 format</th>
      <th>Cd5 format desc</th>
      <th>Default value</th>
      <th>Room 2 notes</th>
      <th>Origin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @origin_fields.each do |origin_field| %>
      <tr>
        <td><%= origin_field.field_name %></td>
        <td><%= origin_field.origin_pic %></td>
        <td><%= origin_field.data_type_origin_field %></td>
        <td><%= origin_field.fmbase_format_type %></td>
        <td><%= origin_field.generic_data_type %></td>
        <td><%= origin_field.decimal_origin_field %></td>
        <td><%= origin_field.mask_origin_field %></td>
        <td><%= origin_field.position_origin_field %></td>
        <td><%= origin_field.width_origin_field %></td>
        <td><%= origin_field.is_key %></td>
        <td><%= origin_field.will_use %></td>
        <td><%= origin_field.has_signal %></td>
        <td><%= origin_field.room_1_notes %></td>
        <td><%= origin_field.cd5_variable_number %></td>
        <td><%= origin_field.cd5_output_order %></td>
        <td><%= origin_field.cd5_variable_name %></td>
        <td><%= origin_field.cd5_origin_format %></td>
        <td><%= origin_field.cd5_origin_format_desc %></td>
        <td><%= origin_field.cd5_format %></td>
        <td><%= origin_field.cd5_format_desc %></td>
        <td><%= origin_field.default_value %></td>
        <td><%= origin_field.room_2_notes %></td>
        <td><%= origin_field.origin %></td>
        <td><%= link_to 'Show', origin_field %></td>
        <td><%= link_to 'Edit', edit_origin_field_path(origin_field) %></td>
        <td><%= link_to 'Destroy', origin_field, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Origin field', new_origin_field_path %>

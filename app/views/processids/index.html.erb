<h1><%= Processid.model_name.human.capitalize %></h1>

<table>
  <thead>
    <tr>
      <th><%= t('activerecord.attributes.processid.process_number') %></th>
      <th><%= t('activerecord.attributes.processid.mnemonic') %></th>
      <th><%= t('activerecord.attributes.processid.routine_name') %></th>
      <th><%= t('activerecord.attributes.processid.var_table_name') %></th>
      <th><%= t('activerecord.attributes.processid.conferencei_rule') %></th>
      <th><%= t('activerecord.attributes.processid.acceptance_percent') %></th>
      <th><%= t('activerecord.attributes.processid.keep_previous_work') %></th>
      <th><%= t('activerecord.attributes.processid.counting_rule') %></th>
      <th><%= t('activerecord.attributes.processid.notes') %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @processids.each do |processid| %>
      <tr>
        <td><%= processid.process_number %></td>
        <td><%= processid.mnemonic %></td>
        <td><%= processid.routine_name %></td>
        <td><%= processid.var_table_name %></td>
        <td><%= processid.conference_rule %></td>
        <td><%= processid.acceptance_percent %></td>
        <td><%= processid.keep_previous_work %></td>
        <td><%= processid.counting_rule %></td>
        <td><%= processid.notes %></td>
        <td><%= link_to t('show'), processid %></td>
        <td><%= link_to t('edit'), edit_processid_path(processid) %></td>
        <td><%= link_to t('destroy'), processid, method: :delete, data: { confirm: t('are_you_sure') } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to "Novo #{Processid.model_name.human.capitalize}", new_processid_path %>
